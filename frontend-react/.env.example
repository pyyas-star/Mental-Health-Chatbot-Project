# Frontend Environment Variables
# Copy this file to .env and update with your production backend URL

# Backend API Base URL (include /api/ at the end)
VITE_BACKEND_BASE_API=http://localhost:8000/api/

# Backend Root URL (without /api/)
VITE_BACKEND_ROOT=http://localhost:8000

# For production, update these to:
# VITE_BACKEND_BASE_API=https://your-backend.onrender.com/api/
# VITE_BACKEND_ROOT=https://your-backend.onrender.com
